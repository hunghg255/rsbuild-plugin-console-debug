{
  "name": "rsbuild-plugin-console-debug",
  "version": "0.0.7",
  "description": "ğŸ“¦ ğŸ“¦ rsbuild plugin console debug",
  "scripts": {
    "build": "tsup && cp -r ./src/core ./dist && cp -r ./src/client ./dist",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "format": "prettier --write .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp -r && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rsbuild",
    "plugin",
    "print"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rsbuild/core": "^0.7.1",
    "@types/node": "^20.12.13",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "eslint-config-relaxed-ts": "^2.0.34",
    "esno": "^0.17.0",
    "prettier": "^3.2.5",
    "tsup": "^7.3.0",
    "typescript": "^5.4.5",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=14",
    "@rsbuild/core": ">=0.2.13"
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@rsbuild/shared": "^0.0.17",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "launch-editor-middleware": "^2.6.1",
    "serve-static": "^1.15.0"
  }
}
